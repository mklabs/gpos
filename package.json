          ],
          "env": {
            "development": {
              "plugins": [
                [
                  "react-transform",
                  {
                    "transforms": [
                      {
                        "transform": "livereactload/babel-transform",
                        "imports": [
                          "react"
                        ]
                      }
                    ]
                  }
                ]
              ]
            }
          }
        }
      ],
      "browserify-css",
      "hyperxify",
      "brfs"
    ]
  },
  "browserify-css": {
    "autoInject": true
